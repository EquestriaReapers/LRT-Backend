<!-- partials/experiencie-item.hbs -->
<div class="row mt-2 pls-not-break-class">
    <div class="col-12 pls-not-break-class">
        <h2 class="mb-1 pls-not-break-class">
            {{#if (is-not-empty temporality)}}
                <span>
                    ({{temporality}})
                </span>
            {{/if}}
            {{title}}
        </h2>
    </div>
    {{#if (is-not-empty where)}}    
        <div class="col-12 pls-not-break-class">
            <span class="normal-text">
                {{where}}
            </span>
        </div>
    {{/if}}
</div>
